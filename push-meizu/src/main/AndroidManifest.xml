<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gg.fanapp.push_meizu">

    <uses-permission android:name="com.meizu.flyme.push.permission.RECEIVE"></uses-permission>
    <permission
        android:name="com.fan.app.push.permission.MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.fan.app.push.permission.MESSAGE"></uses-permission>

    <uses-permission android:name="com.meizu.c2dm.permission.RECEIVE" />

    <permission
        android:name="com.fan.app.permission.C2D_MESSAGE"
        android:protectionLevel="signature"></permission>
    <uses-permission android:name="com.fan.app.permission.C2D_MESSAGE" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name">
        <!-- push应用定义消息receiver声明 -->
        <receiver android:name="com.gg.fanapp.push_meizu.MeizuPushReciver">
            <intent-filter>
                <!-- 接收push消息 -->
                <action android:name="com.meizu.flyme.push.intent.MESSAGE" />
                <!-- 接收register消息-->
                <action android:name="com.meizu.flyme.push.intent.REGISTER.FEEDBACK" />
                <!-- 接收unregister消息-->
                <action android:name="com.meizu.flyme.push.intent.UNREGISTER.FEEDBACK" />

                <action android:name="com.meizu.c2dm.intent.REGISTRATION" />
                <action android:name="com.meizu.c2dm.intent.RECEIVE" />

                <category android:name="com.fan.app"></category>
            </intent-filter>
        </receiver>
    </application>

</manifest>
